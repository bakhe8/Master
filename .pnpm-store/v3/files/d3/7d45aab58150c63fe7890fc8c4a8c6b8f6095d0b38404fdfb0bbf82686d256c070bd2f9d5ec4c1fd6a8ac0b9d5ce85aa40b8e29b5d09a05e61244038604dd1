import { Specs } from './interfaces';
export declare class SallaMetadata {
    constructor();
    host: HTMLElement;
    /**
     * The entity type.
     */
    entity: string;
    /**
     * The id of the product/the endity to which the specs are going to be fetched for.
     */
    entityId: number | number[] | string;
    specs: Specs[];
    download: string;
    private getValue;
    componentWillLoad(): Promise<any>;
    render(): any;
}
