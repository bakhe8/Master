/**
 * @slot add-to-cart-label - Add to cart label.
 */
export declare class SallaProductCard {
    /**
   *  Product information.
   */
    product: string | object;
    /**
     *  Horizontal card.
     */
    horizontal: boolean;
    /**
     *  Support shadow on hover.
     */
    shadowOnHover: boolean;
    /**
     *  Hide add to cart button.
     */
    hideAddBtn: boolean;
    /**
     *  Full image card.
     */
    fullImage: boolean;
    /**
     *  Minimal card.
     */
    minimal: boolean;
    /**
     *  Special card.
     */
    isSpecial: boolean;
    /**
     *  Show quantity.
     */
    showQuantity: boolean;
    host: HTMLElement;
    productData: any;
    fitImageHeight: boolean;
    remained: string;
    outOfStock: string;
    donationAmount: string;
    startingPrice: string;
    addToCart: string;
    placeholder: string;
    private pie;
    private wishlistBtn;
    private addBtn;
    constructor();
    private initCircleBar;
    private toggleFavoriteIcon;
    private formatDate;
    private getProductBadge;
    getPriceFormat(price: any): any;
    private getProductPrice;
    render(): any;
    componentDidLoad(): void;
}
