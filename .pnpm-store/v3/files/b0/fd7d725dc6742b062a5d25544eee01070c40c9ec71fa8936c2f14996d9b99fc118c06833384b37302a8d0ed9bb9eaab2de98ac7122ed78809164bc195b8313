import { TieredOfferTier } from './interfaces';
export declare class TierUtils {
    /**
     * Find tier by index
     */
    static findTierByIndex(tiers: TieredOfferTier[], index: number): TieredOfferTier | null;
    /**
     * Find tier by tier name
     */
    static findTierByName(tiers: TieredOfferTier[], tierName: string): TieredOfferTier | null;
    /**
     * Get tier name by index
     */
    static getTierNameByIndex(tiers: TieredOfferTier[], index: number): string | undefined;
    /**
     * Find current tier index based on current value
     */
    static findCurrentTierIndex(currentValue: number, tiers: TieredOfferTier[]): number;
}
