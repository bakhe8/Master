export type Source = "products" | "categories" | "json";
export type ReviewType = "all" | "store" | "products";
export type SortingOption = "top_rating" | "random" | "latest";
export interface Review {
    rating?: number;
    content?: string;
    status?: string;
    created_at?: CreatedAt;
    replies?: any[];
    text?: string;
    type?: string;
    avatar?: string;
    name?: string;
    stars?: number;
    is_pending?: boolean;
    has_order?: boolean;
    kind?: string;
    session_id?: string;
    customer_id?: number;
    city?: string;
    date?: number;
}
export interface CreatedAt {
    date?: string;
    timezone_type?: number;
    timezone?: string;
}
